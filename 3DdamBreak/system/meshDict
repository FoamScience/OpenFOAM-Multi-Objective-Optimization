FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      meshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mesher pMesh;

surfaceFile "geometry/edges.stl";

minCellSize 0.030;
maxCellSize 4.000;

renameBoundary
{
    defaultName		obstacle;
    defaultType		wall;

    newPatchNames
    {
        "OpenSCAD_Model_1"
        {
            newName     atmosphere;
            type     patch;
        }
        "OpenSCAD_Model_(0|2|3|4|5)"
        {
            newName     walls;
            type     wall;
        }
    }
}
