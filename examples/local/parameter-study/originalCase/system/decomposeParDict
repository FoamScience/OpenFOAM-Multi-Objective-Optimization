FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 64;

method          hierarchical;

hierarchicalCoeffs
{
    n       ( 4 4 4);
    delta   0.001;
    order   xyz;
}

