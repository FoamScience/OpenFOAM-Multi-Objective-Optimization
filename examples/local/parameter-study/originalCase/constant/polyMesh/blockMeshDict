zmax 0;


vertices        
(
($xmin $ymin $zmin) //0
($xmax $ymin $zmin) //1
($xmax $ymax $zmin) //2
($xmin $ymax $zmin) //3
($xmin $ymin $zmax) //4
($xmax $ymin $zmax) //5
($xmax $ymax $zmax) //6
($xmin $ymax $zmax) //7

);

blocks          
(
   hex (0 1 2 3 4 5 6 7) (100 20 25) simpleGrading (1 1 1) 
   
);

