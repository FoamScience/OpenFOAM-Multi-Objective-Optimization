FoamFile
{
    version     2.0;
    format      ascii;
    class       uniformDimensionedVectorField;
    location    "constant";
    object      g;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

/*inclination(deg)	0;*/
//value           (0 0 0);
g 4.1458 -8.89087;

/*inclination(deg)	20*/
value           ($g 0);



